<%- include('ek/header', {bot, user, path}) %>
<div class="container">
  <div align="center" class="jumbotron">
    <%
    var txt = `Click to see ${kisi.username}#${kisi.discriminator} profile`
    if (user) {
      if (user.id === kisi.id) {
        var txt = `Click to view profile`
      }
    }
    %>
       <img src="https://cdn.discordapp.com/avatars/<%=kisi.id%>/<%=kisi.avatar%>.png?size=512" class="rounded-circle panell">
       <h5 style="color:#fff"><%=kisi.username%>#<%=kisi.discriminator%></h5>
       <a href="/kullanici/<%=kisi.id%>/profil" class="btn btn-default" style="background-color:#333; color:#fff"><%=txt%></a>
       <% if (user) { %>
       <% if (user.id === kisi.id) { %>
       <a href="/kullanici/<%=kisi.id%>/panel" class="btn btn-default" style="background-color:#333; color:#fff">Click to go to panel</a>
       <% } %>
       <% } %>
  </div>
</div>
</div></div>
<% include ./ek/footer %>